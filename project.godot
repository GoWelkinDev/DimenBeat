; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="DimenBeat"
config/name_localized={
"zh_CN": "维度的节奏",
"zh_HK": "維度的節奏",
"zh_TW": "維度的節奏"
}
config/description="DimenBeat Demo Version"
run/main_scene="res://control.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/show_image=false
config/icon="res://icon.png"

[autoload]

Global="*res://scripts/Global.gd"
GlobalSteam="*res://scripts/steamworks/steam.gd"
Verify="res://scripts/steamworks/verify.gd"
Language="*res://scripts/system/language.gd"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/viewport_width=680
window/size/viewport_height=640
window/size/mode=3
window/size/resizable=false
window/size/borderless=true

[editor_plugins]

enabled=PackedStringArray("res://addons/licenses/plugin.cfg")

[internationalization]

locale/locale_filter_mode=1

[memory]

limits/message_queue/max_size_mb=2048

[rendering]

textures/canvas_textures/default_texture_filter=2
textures/vram_compression/import_etc2_astc=true
textures/default_filters/anisotropic_filtering_level=3
textures/decals/filter=4
textures/light_projectors/filter=4
anti_aliasing/quality/msaa_2d=2
viewport/hdr_2d=true
anti_aliasing/quality/use_taa=true
